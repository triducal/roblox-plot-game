{
    "name": "RobloxProjectTemplate",
    "emitLegacyScripts": false,
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "Packages": {
                "$path": "Packages"
            },
            "Client": {
                "$path": "src/Client"
            },
            "Shared": {
                "$path": "src/Shared"
            },
            "Assets": {
                "$path": "assets"
            }
        },
        "ServerScriptService": {
            "Server": {
                "$path": "src/Server"
            },
            "ServerPackages": {
                "$path": "ServerPackages"
            }
        }
    }
}
